{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico}",
      "/*.{css,scss,js,png,jpg,gif,ico}"
    ]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".svg": "image/svg+xml"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/ebd/*/*.svg",
      "headers": {
        "Content-Type": "image/svg+xml"
      }
    }
  ]
}
