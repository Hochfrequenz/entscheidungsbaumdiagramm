{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico}",
      "/*.{css,scss,js,png,jpg,gif,ico}"
    ]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".svg": "image/svg+xml"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/ebd/*/*",
      "headers": {
        "Cache-Control": "no-cache",
        "Content-Type": "image/svg+xml"
      }
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  }
}
