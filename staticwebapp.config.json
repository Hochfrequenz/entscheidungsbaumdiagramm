{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_app/*",
      "/static/*",
      "/ebd/*/*.svg",
      "/favicon.ico",
      "/favicon/*",
      "/*.{png,jpg,gif,svg}",
      "/logo.svg"
    ]
  },
  "routes": [
    {
      "route": "/__data.json*",
      "headers": {
        "Content-Type": "application/json"
      }
    },
    {
      "route": "/ebd/*",
      "serve": "/index.html"
    }
  ],
  "mimeTypes": {
    ".svg": "image/svg+xml",
    ".json": "application/json"
  }
}
