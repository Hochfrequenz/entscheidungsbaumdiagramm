{
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": [
      "/images/*.{png,jpg,gif,ico}",
      "/*.{css,scss,js,png,jpg,gif,ico,svg}"
    ]
  },
  "routes": [
    {
      "route": "/ebd/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "route": "/*",
      "serve": "/404.html",
      "statusCode": 404
    }
  ],
  "mimeTypes": {
    ".json": "text/json",
    ".svg": "image/svg+xml"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  }
}
