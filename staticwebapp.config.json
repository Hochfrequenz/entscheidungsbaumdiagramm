{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/_app/*",
      "/static/*",
      "/favicon.ico",
      "/favicon/*",
      "/logo.svg",
      "/*.{png,jpg,gif,svg}"
    ]
  },
  "routes": [
    {
      "route": "/*.json",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "no-store",
        "X-Content-Type-Options": "nosniff"
      }
    },
    {
      "route": "/ebd/*/index.json",
      "headers": {
        "Content-Type": "application/json",
        "Cache-Control": "no-store",
        "X-Content-Type-Options": "nosniff"
      }
    },
    {
      "route": "/ebd/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "application/json",
    ".svg": "image/svg+xml"
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff"
  }
}
