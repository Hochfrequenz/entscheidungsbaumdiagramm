{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/favicon.ico",
      "/favicon/*",
      "/*.{png,jpg,gif,svg}",
      "/logo.svg"
    ]
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/ebd",
      "rewrite": "/index.html"
    },
    {
      "route": "/ebd/*",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".svg": "image/svg+xml"
  }
}
